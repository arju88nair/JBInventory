
  CREATE TABLE "OPAC"."PROCUREMENT_TYPE" 
   (	"ID" NUMBER, 
	"NAME" VARCHAR2(20 BYTE), 
	"CODE" VARCHAR2(20 BYTE)
   ) PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT)
  TABLESPACE "USERS" ;
 





  CREATE TABLE "OPAC"."VENDOR_PARSING" 
   (	"VENDOR" VARCHAR2(100 BYTE) NOT NULL ENABLE, 
	"COLUMN_ISBN" VARCHAR2(20 BYTE), 
	"COLUMN_CUR" VARCHAR2(20 BYTE), 
	"COLUMN_PRICE" VARCHAR2(20 BYTE), 
	"COLUMN_QUANTITY" VARCHAR2(20 BYTE), 
	"COLUMN_ISBN10" VARCHAR2(20 BYTE), 
	"VENDOR_ID" NUMBER
   ) PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT)
  TABLESPACE "USERS" ;
 



  CREATE TABLE "OPAC"."VENDOR_STOCK_DETAILS" 
   (	"VENDOR_ID" NUMBER NOT NULL ENABLE, 
	"ISBN" VARCHAR2(25 BYTE), 
	"CURRENCY" VARCHAR2(20 BYTE), 
	"PRICE" VARCHAR2(20 BYTE), 
	"QUANTITY" NUMBER
   ) PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT)
  TABLESPACE "USERS" ;
 


  CREATE TABLE "OPAC"."TEMP_TEST" 
   (	"TITLE_ID" VARCHAR2(20 BYTE), 
	"QUANTITY" NUMBER
   ) PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT)
  TABLESPACE "USERS" ;
 



  CREATE TABLE "OPAC"."BATCH" 
   (	"ID" NUMBER NOT NULL ENABLE, 
	"NAME" VARCHAR2(20 BYTE), 
	"DESCRIPTION" VARCHAR2(255 BYTE), 
	"FROM_DATE" DATE, 
	"TO_DATE" DATE, 
	"STATUS" VARCHAR2(20 BYTE), 
	"CREATED_AT" DATE, 
	"UPDATED_AT" DATE, 
	"ACTIVE" NUMBER(1,0) DEFAULT 1, 
	"PROCUREMENT_TYPE_ID" NUMBER
   ) PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT)
  TABLESPACE "USERS" ;
 

  CREATE OR REPLACE TRIGGER "OPAC"."BATCH_TRG1" 
BEFORE INSERT ON OPAC.BATCH 
FOR EACH ROW 
BEGIN
  <<COLUMN_SEQUENCES>>
  BEGIN
    NULL;
  END COLUMN_SEQUENCES;
END;
/
ALTER TRIGGER "OPAC"."BATCH_TRG1" ENABLE;
 

  CREATE OR REPLACE TRIGGER "OPAC"."BATCH_TRG2" 
BEFORE INSERT ON BATCH 
FOR EACH ROW 
BEGIN
  <<COLUMN_SEQUENCES>>
  BEGIN
    NULL;
  END COLUMN_SEQUENCES;
END;
/
ALTER TRIGGER "OPAC"."BATCH_TRG2" ENABLE;
 
